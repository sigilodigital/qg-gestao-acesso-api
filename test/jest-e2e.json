{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": "..",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^src/(.*)": "<rootDir>/src/$1",
    "^@sd-root(|/.*)$": "<rootDir>$1",
    "@libs/common/(.*)": "<rootDir>/libs/common/src/$1",
    "@libs/common": "<rootDir>/libs/common/src",
    "@libs/audit/(.*)": "<rootDir>/libs/audit/src/$1",
    "@libs/audit": "<rootDir>/libs/audit/src"
  }
}